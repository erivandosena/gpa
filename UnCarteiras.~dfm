  object IBQuery_Dependente: TIBQuery
    Database = Dm.IBDatabase
    Transaction = Dm.IBTransaction
    ForcedRefresh = True
    SQL.Strings = (
      'select COD_DEPE, CODIGO, NOME_DEPENDENTE, PARENTESCO,'
      '(select count (CODIGO) from DEPENDE'
      'where CODIGO >= '#39'0001'#39' and CODIGO <= '#39'0000'#39')'
      'from DEPENDE, CLIENTES'
      'where CODIGO >= '#39'0001'#39' and CODIGO <= '#39'0000'#39' AND'
      'CODIGO = CLIENTES.CODIGO')
    Left = 520
    Top = 112
    object IBQuery_DependenteCODIGO: TIBStringField
      Alignment = taCenter
      FieldName = 'CODIGO'
      Origin = '"DEPENDE"."CODIGO"'
      Visible = False
      Size = 11
    end
    object IntegerField2: TIntegerField
      Alignment = taCenter
      DisplayLabel = 'C'#243'digo'
      FieldName = 'COD_DEPE'
      Origin = '"DEPENDE"."COD_DEPE"'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
      DisplayFormat = '####0000'
    end
    object IBStringField10: TIBStringField
      DisplayLabel = 'Dependente/Usu'#225'rio Empresa'
      FieldName = 'NOME_DEPENDENTE'
