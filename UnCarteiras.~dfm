  object IBQuery_Titular: TIBQuery
    Database = Dm.IBDatabase
    Transaction = Dm.IBTransaction
    ForcedRefresh = True
    SQL.Strings = (
      
        'select CIDADE, CODIGO, CPF, ENDERECO, ESTADO, NOME_CLIENTE, RG, ' +
        'TE,'
      '(select count (CODIGO) from CLIENTES'
      'where CODIGO >= '#39'0001'#39' and CODIGO <= '#39'0000'#39')'
      'from CLIENTES'
      'where CODIGO >= '#39'0001'#39' and CODIGO <= '#39'0000'#39
      'order by CODIGO')
    Left = 88
    Top = 112
    object IBQuery_TitularCODIGO: TIBStringField
      Alignment = taCenter
      DisplayLabel = 'Cod. Tit.'
      FieldName = 'CODIGO'
      Origin = '"DEPENDE"."CODIGO"'
      Size = 11
    end
